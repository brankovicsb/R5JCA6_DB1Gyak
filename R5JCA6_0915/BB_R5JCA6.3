#include <stdlib.h>
#include <stdio.h>
#include <string.h>

int main()
{
    char szoveg[1000];
    FILE *fp1;
    fp1 = fopen("szoveg.txt", "r");

    if(fp1 != NULL)
    {
        while(fgets(szoveg, sizeof(szoveg), fp1) != NULL)
        {
            fgets(szoveg, sizeof(szoveg), fp1);
            puts(szoveg);
        }
    }
    else
    {
        printf("File hiba!");
    }
    fclose(fp1);
    return 0;
}
